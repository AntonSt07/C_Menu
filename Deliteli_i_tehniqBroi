int n,i,count;
    do{
        printf("n = ");
        scanf("%d", &n);
    }
    while(n < 2);

    i = 1;
    count = 0;
    while(n >= i){
        if(n % i == 0){
            printf("%d\n", i);
            i++;
            count++;
        }
        else{
            i++;
        }
    }
    printf("Ima %d delitelq", count);